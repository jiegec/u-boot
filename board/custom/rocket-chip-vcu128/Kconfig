if TARGET_ROCKET_CHIP_VCU128

config SYS_BOARD
	default "rocket-chip-vcu128"

config SYS_VENDOR
	default "custom"

config SYS_CPU
	default "generic"

config SYS_CONFIG_NAME
	default "rocket-chip-vcu128"

config SYS_TEXT_BASE
	default 0x80000000

config BOARD_SPECIFIC_OPTIONS # dummy
	def_bool y
	select GENERIC_RISCV
	imply SMP
	imply BOARD_LATE_INIT
	imply PCI_INIT_R
	imply CMD_POWEROFF
	imply CMD_PING
	imply SYS_NS16550
	imply SYSRESET
	imply SYSRESET_CMD_POWEROFF
	imply SYSRESET_SYSCON

endif
